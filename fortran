program primos
    implicit none
        integer n, x, i, r, q, c
        x=2
        c=1
            do i=1, 100, +1
                r=mod(x,2)
                q=mod(x,3)
            if (r>0) then
                if (q>0) then
                    write (*,*) x
                end if
            end if 
                x=x+1
            end do    
    end program            
        
